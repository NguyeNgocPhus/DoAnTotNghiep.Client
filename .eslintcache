ath;\n  }\n  return (\n    <>\n      <Routes>\n        <Route path=\"/\" element={<HomePage></HomePage>}></Route>\n        <Route path=\"/admin/workflow\" element={<WorkflowDetail></WorkflowDetail>}></Route>\n        <Route path=\"/user/signin\" element={<UserSignIn></UserSignIn>}></Route>\n        <Route path=\"/user/signup\" element={<UserSignUp></UserSignUp>}></Route>\n        <Route path=\"/user/getpassword\" element={<GetPassword></GetPassword>}></Route>\n        {listTypeProduct.state === REQUEST_STATE.SUCCESS && listTypeProduct.data.map((type) => {\n          return (\n            <>\n              <Route path={`/${type.nameSlug}`} element={<PageProduct></PageProduct>}></Route>\n              <Route path={`/${type.nameSlug}/:productDetail`} element={<ProductDetail></ProductDetail>}></Route>\n            </>\n          )\n        })}\n      </Routes>\n\n      <PrivateRouter path=\"/profile\" element={<Profile></Profile>}></PrivateRouter>\n\n\n    </>\n\n  );\n}\n\nexport default App;\n",["227","228"],"/Users/phunn/Documents/Code/DoAn/DoAn.Client/src/app-config/constants.js",["229"],"import moment from \"moment\";\n\nexport const NOTIFICATION = {\n    SUCCESS: \"Thành công\",\n    INFO: \"Thông tin\",\n    ERROR: \"Lỗi\",\n};\n\nexport const REQUEST_STATE = {\n    SUCCESS: \"SUCCESS\",\n    ERROR: \"ERROR\",\n    REQUEST: \"REQUEST\",\n    UNMOUNT: \"UNMOUNT\",\n    INVALID: \"INVALID\",\n    PROCESSING: \"Đang xử lý\",\n};\n\nexport const MESSAGE = {\n    SUCCESS: \"success\",\n    ERROR: \"error\",\n};\n\nexport const FORMAT_DATE = {\n    DAY_MONTH_YEAR: \"DD/MM/YYYY\",\n    YEAR_MONTH_DAY: \"YYYY-MM-DD\"\n};\n\nexport const CONST_FILE = {\n    Image: \"image\",\n    Video: \"video\",\n    All: \"all\"\n};\n\nexport const TypeFormInputResponse = {\n    SELECT: \"select\",\n    TEXT: \"text\"\n};\n\n","/Users/phunn/Documents/Code/DoAn/DoAn.Client/src/store/auth/use-my-profile.js",[],"/Users/phunn/Documents/Code/DoAn/DoAn.Client/src/store/type-product/use-type-